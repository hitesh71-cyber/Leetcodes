class Solution {
public:
    int minMoves(vector<int>& nums) {
        int x=0,maxn=nums[0];
        int add=0;
        for(int i=1;i<nums.size();i++)
            {
                if(maxn<nums[i])
                    maxn=nums[i];
            }
        for(int i=0;i<nums.size();i++)
            {
                x=maxn-nums[i];
                add=add+x;
            }
        return add;
    }
};
